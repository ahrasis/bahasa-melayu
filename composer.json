	"authors": [
		{
		  "name": "Hj Ahmad Rasyid Hj Ismail",
		  "email": "ahrasis@gmail.com",
		  "homepage": "https://discuss.flarum.org/d/3787-malaysian-language-pack-for-flarum"
		}
	],
	"support": {
	  "source": "https://github.com/ahrasis/bahasa-melayu",
