{
	"name": "ahrasis/bahasa-melayu",
	"type": "flarum-extension",
	"description": "Pakej Bahasa Melayu untuk Flarum",
	"keywords": [
		"flarum",
		"locale",
		"ahrasis",
		"bahasa",
		"bahasa-malaysia",
		"bahasa-melayu",
		"malaysia",
		"melayu"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Hj Ahmad Rasyid Hj Ismail",
			"email": "ahrasis@gmail.com",
			"homepage": "https://discuss.flarum.org/d/3787-malaysian-language-pack-for-flarum"
		}
	],
	"support": {
		"source": "https://github.com/ahrasis/bahasa-melayu",
		"issues": "https://github.com/ahrasis/bahasa-melayu/issues",
		"site": "https://discuss.flarum.org/d/3787-malaysian-language-pack-for-flarum"
	},
	"require": {
		"flarum/core": "^1.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
